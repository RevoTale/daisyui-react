{
  "author": {
    "name": "rusted_love",
    "email": "rusted@revotale.com",
    "url": "https://github.com/rusted-love"
  },
  "main": "src/index.ts",
  "license": "MIT",
  "private": false,
  "sideEffects": false,
  "publishConfig": {
    "provenance": true,
    "access": "public",
    "main": "index.js",
    "directory": "dist",
    "exports": {
      "./package.json": "./package.json",
      ".": {
        "import": "./index.js",
        "require": "./index.cjs",
        "types": "./index.d.ts"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RevoTale/daisyui-react.git"
  },
  "bugs": {
    "url": "https://github.com/RevoTale/next-scroll-restorer/issues"
  },
  "packageManager": "pnpm@8.7.5",
  "name": "daisyui-react",
  "version": "0.0.1",
  "description": "daisyui-react simplify development with DaisyUI + latest React features",
  "type": "module",
  "exports": {
    "import": "./dist/daisyui-react.modern.js",
    "require": "./dist/daisyui-react.cjs",
    "default": "./dist/daisyui-react.modern.js",
    "types": "./dist/index.d.ts"
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "gbuild . && tsc --emitDeclarationOnly",
    "types:check": "tsc --noEmit",
    "changeset:version": "changeset version && git add --all",
    "changeset:publish": "changeset publish",
    "prepare": "pnpm build",
    "test": "pnpm types:check",
    "lint": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "peerDependencies": {
    "daisyui": "^4.6.0",
    "react": ">=16",
    "react-dom": ">=16",
    "tailwindcss": ">=3.2.7"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "changeset": "^0.2.6",
    "clsx": "^2.1.0",
    "eslint-config-grisaia-react": "^0.6.15",
    "eslint-config-grisaia-typescript": "^0.6.14",
    "glob": "^10.3.10",
    "grisaia-package-builder": "^0.1.15",
    "postcss": "^8.4.33",
    "prettier": "^3.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.2.0",
    "typescript": "^5.3.3"
  }
}
